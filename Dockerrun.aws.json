{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "rabbitmq-config",
      "host": {
        "sourcePath": "/root/conf"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management",
      "essential": true,
      "memory": 512,
      "mountPoints": [
        {
          "sourceVolume": "rabbitmq-config",
          "containerPath": "/etc/rabbitmq",
          "readOnly": true
        }
      ]
    },
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 15672
        }
      ],
      
      "mountPoints": [
        {
          "sourceVolume": "rabbitmq-config",
          "containerPath": "/etc/rabbitmq",
          "readOnly": true
        }
      ]
    }
  ]
}